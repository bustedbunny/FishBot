<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="FishBotMAUI.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:FishBotMAUI"
       xmlns:pages="clr-namespace:FishBotMAUI.Pages">

    <TabBar>

        <ShellContent Title="Home"
                      ContentTemplate="{DataTemplate local:MainPage}"
                      Route="MainPage" />

        <ShellContent Title="Fish list"
                      ContentTemplate="{DataTemplate pages:FishPage}"
                      Route="FishPage" />

    </TabBar>

</Shell>
